<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="120.0" prefWidth="560.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="primary_page.controller.CardController">
   <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <ImageView fx:id="imageView" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </ImageView>
      <VBox alignment="CENTER_LEFT">
         <Text fx:id="title" onMouseClicked="#clickTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Title">
            <font>
               <Font size="20.0" />
            </font>
            <VBox.margin>
               <Insets left="10.0" />
            </VBox.margin>
         </Text>
         <Text fx:id="time" strokeType="OUTSIDE" strokeWidth="0.0" text="Time">
            <font>
               <Font size="16.0" />
            </font>
            <VBox.margin>
               <Insets left="10.0" />
            </VBox.margin>
         </Text>
         <Label text="source">
            <font>
               <Font size="16.0" />
            </font>
            <padding>
               <Insets left="10.0" right="10.0" />
            </padding>
         </Label>
      </VBox>
   </HBox>
</AnchorPane>
