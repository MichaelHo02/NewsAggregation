<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER" spacing="10.0" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="primary_page.controller.NavigationController">
   <Button fx:id="prevPage" focusTraversable="false" mnemonicParsing="false" onMouseClicked="#setCurrentButton" text="&lt;">
      <HBox.margin>
         <Insets />
      </HBox.margin>
   </Button>
   <Button fx:id="page1" focusTraversable="false" mnemonicParsing="false" onMouseClicked="#setCurrentButton" text="1">
      <HBox.margin>
         <Insets />
      </HBox.margin>
   </Button>
   <Button fx:id="page2" focusTraversable="false" mnemonicParsing="false" onMouseClicked="#setCurrentButton" text="2">
      <HBox.margin>
         <Insets />
      </HBox.margin>
   </Button>
   <Button fx:id="page3" focusTraversable="false" mnemonicParsing="false" onMouseClicked="#setCurrentButton" text="3">
      <HBox.margin>
         <Insets />
      </HBox.margin>
   </Button>
   <Button fx:id="page4" focusTraversable="false" mnemonicParsing="false" onMouseClicked="#setCurrentButton" text="4">
      <HBox.margin>
         <Insets />
      </HBox.margin>
   </Button>
   <Button fx:id="page5" focusTraversable="false" mnemonicParsing="false" onMouseClicked="#setCurrentButton" text="5">
      <HBox.margin>
         <Insets />
      </HBox.margin>
   </Button>
   <Button fx:id="nextPage" focusTraversable="false" mnemonicParsing="false" onMouseClicked="#setCurrentButton" text="&gt;">
      <HBox.margin>
         <Insets />
      </HBox.margin>
   </Button>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <styleClass>
      <String fx:value="column_row_background" />
      <String fx:value="shadow" />
   </styleClass>
</HBox>
